@using CitizenJournalismNetworkServer.Models
@using CitizenJournalismNetworkServer.Helpers
@model Feed
{
    "title": "@Model.Title",
    "authors": @(JsonHelper.RenderJsonCollection<Person>("_Author", Model.Authors, this)),
    "categories": @(JsonHelper.RenderJsonCollection<Category>("_Category", Model.Categories, this)),
    "contributors": @(JsonHelper.RenderJsonCollection<Person>("_Contributor", Model.Contributors, this)),
    "generator": @Html.Partial("_Generator", Model.Generator),
    "icon": "@Model.IconUri",
    "id": "@Model.AtomId",
    "links": @(JsonHelper.RenderJsonCollection<Link>("_Link", Model.Links, this)),
    "logo": "@Model.LogoUri",
    "rights": "@Model.Rights",
    "subtitle": "@Model.Subtitle",
    "updated": "@Model.DateUpdated.ToString()",
    "entries": @(JsonHelper.RenderJsonCollection<Entry>("_Entry", Model.Entries, this))
}
