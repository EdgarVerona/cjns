@using CitizenJournalismNetworkServer.Models
@using CitizenJournalismNetworkServer.Helpers
@model Entry
           
"authors": @(JsonHelper.RenderJsonCollection<Person>("_atomAuthor", Model.Authors, this.Page)),
"categories": @(JsonHelper.RenderJsonCollection<Category>("_atomCategory", Model.Categories, this.Page)),
"contributors": @(JsonHelper.RenderJsonCollection<Person>("_atomContributor", Model.Contributors, this.Page)),
"id": "@Model.AtomId",
"links": @(JsonHelper.RenderJsonCollection<Link>("_atomLink", Model.Links, this.Page)),
"content": @Html.Partial("_atomContent", Model.Content),
"published": "@Model.DatePublished.ToString()",
"rights": "@Model.Rights",
"summary": "@Model.Summary",
"title": "@Model.Title",
"updated": "@Model.DateUpdated.ToString()"